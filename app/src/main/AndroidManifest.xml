<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uditsetia.assignment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="66a1d7fbe62e250fab7304a26b9dd29538168f43" />

        <activity android:name=".LoginComplete">

            <intent-filter>
                <action android:name="android.intent.action.LoginComplete" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name=".DisplayReferalUsage">

            <intent-filter>
                <action android:name="android.intent.action.DisplayReferalUsage" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


    </application>


    <uses-permission android:name="android.permission.INTERNET" />
</manifest>